    }

    @media screen and (max-width: 992px) {
      .it-header-center-wrapper .it-header-center-content-wrapper {
        flex-direction: column;
        align-items: start;
        padding-block: 8px;
      }
      .it-header-center-wrapper .it-header-center-content-wrapper .it-brand-wrapper .icon {
        width: 64px !important;
        height: 64px !important;
      }
      .it-header-center-wrapper .it-header-center-content-wrapper .it-right-zone {
        align-items: start;
      }
      .it-header-center-wrapper .it-header-center-content-wrapper .it-right-zone p {
        font-size: 0.875rem;
        line-height: 1.3;
      }
    }

    section.metrics .donut-container {
      margin: 1.5rem 0;
      display: flex;
      justify-content: space-evenly;
      align-items: center;
    }

    section.metrics .legend-item::before {
      content: "";
      display: inline-block;
      width: 20px;
      height: 20px;
      position: relative;
      top: 3.5px;
      border-radius: 100%;
      background-color: #B42D43;
      margin-right: 12px;
    }

    section.metrics .legend-item.average::before {
      background-color: #976700;
    }

    section.metrics .legend-item.pass::before {
      background-color: #5CC98D;
    }

    .metrics-container {
      display: grid;
      grid-template-columns: repeat(1, 1fr);
      gap: 1rem 2rem;
      padding-bottom: 2rem;
    }

    .metrics-container .metric-wrapper {
      padding-bottom: 1rem;
      border-bottom: 1px solid #c5c7c9;
    }

    .metrics-container .metric-wrapper .d-flex {
      justify-content: space-between;
      margin-bottom: 0.5rem;
    }

    .metrics-container .metric-wrapper p {
      margin: 0;
    }

    .metrics-container .metric-wrapper p.result {
      font-size: var(--fs-32px);
      font-weight: 700;
    }

    .metrics-container .metric-wrapper .pass {
      color: #5CC98D;
      stroke: #5CC98D;
    }

    .metrics-container .metric-wrapper .fail {
      color: #B42D43;
      stroke: #B42D43;
    }

    .metrics-container .metric-wrapper .average {
      color: #976700;
      stroke: #976700;
    }

    .metrics-container .metric-wrapper .icon {
      width: 28px;
      height: 28px;
      position: relative;
      bottom: 2px;
      margin-right: 6px;
    }

    iframe {
      display: block;
    }

    .table-wrapper {
      overflow: auto;
    }

    .table-wrapper .table {
      border-collapse: separate;
      border-spacing: 0;
      font-size: 1rem;
    }

    .table-wrapper::-webkit-scrollbar {
      -webkit-appearance: none;
      width: 8px;
    }

    .table-wrapper::-webkit-scrollbar {
      height: 8px;
    }

    .table-wrapper::-webkit-scrollbar-track {
      background: #f1f1f1;
    }

    .table-wrapper::-webkit-scrollbar-thumb {
      background-color: #888;
      border-radius: 10px;
    }

    .table-wrapper .table thead {
      font-weight: 700;
      position: sticky;
      top: 0;
      background-color: #ffffff;
    }

    .table-wrapper .table thead th {
      border-bottom: 3px solid var(--bs-secondary);
    }

    .table-wrapper .table tr th,
    .table-wrapper .table tr td {
      padding: 0.75rem;
      min-width: 150px;
    }

    .table-wrapper .table tr th:first-child,
    .table-wrapper .table tr td:first-child {
      padding-left: 1.5rem;
    }

    .table-wrapper .table tr th:last-child,
    .table-wrapper .table tr td:last-child {
      padding-right: 1.5rem;
    }

    @media screen and (max-width: 992px) {
      .audit-content {
        padding: 2rem 0 1rem;
      }

      .audit-content .audit-title svg {
        position: initial;
        margin-top: -5px;
      }
    }

    @media screen and (max-width: 768px) {
      .it-header-center-wrapper {
        height: auto;
        padding-block: 8px;
      }

      .it-header-center-wrapper .it-header-center-content-wrapper {
        flex-direction: column;
        align-items: flax-start;
        row-gap: 8px;
      }

      .it-header-center-wrapper .it-header-center-content-wrapper .it-right-zone {
        flex-direction: row;
        column-gap: 16px;
      }
    }

    @media screen and (max-width: 576px) {
      .it-header-center-wrapper {
        padding-inline: 0;
      }

      .it-header-center-wrapper .it-header-center-content-wrapper .it-brand-wrapper {
        word-break: break-all;
      }

      .it-header-center-wrapper .it-header-center-content-wrapper .it-right-zone {
        flex-direction: column;
        align-items: flex-start;
      }

      .it-header-center-wrapper .it-header-center-content-wrapper .it-right-zone p {
        font-size: 0.875rem;
        line-height: 1.3;
      }
    }

    /*# sourceMappingURL=style.css.map */
  </style>
</head>

<body>
<%- include('partials/icon'); %>
<%- include('partials/header', { url_comune, date, crawler_version }); %>

<main class="container pt-5 pb-5">
  <%- include('partials/top-text'); %>

  <div class="cards-container d-flex justify-content-between gap-5">
    <%- include('partials/card-pass', { results }); %>
    <%- include('partials/card-stats', { results }); %>
  </div>

  <div class="legend">
    <b aria-hidden="true">Legenda:</b>
    <ul class="legend" role="list" title="Legenda risultato">
      <li class="legend-item">
        <div class="circle pass" aria-hidden="true"></div>
        <p id="legend-pass">Il controllo del criterio è superato</p>
      </li>

      <li class="legend-item">
        <div class="circle fail" aria-hidden="true"></div>
        <p id="legend-fail">Il controllo del criterio è fallito</p>
      </li>

      <li class="legend-item">
        <div class="circle error" aria-hidden="true"></div>
        <p id="legend-error">Il controllo del criterio ha generato un errore</p>
      </li>
    </ul>
  </div>

  <div class="accordion" id="collapse">
    <% if (pagesInError.length >0) { %>
      <div class="accordion-item">
        <h2 class="accordion-header" id="accordion-header-failed-audits">
          <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#accordion-collapse-error-audits" aria-expanded="true" aria-controls="accordion-collapse-error-audits">
            Controlli che hanno generato un errore in fase di scansione
          </button>
        </h2>
        <div id="accordion-collapse-error-audits" class="accordion-collapse collapse show" role="region" aria-labelledby="accordion-header-error-audits">
          <%- include('partials/audit/table_errors', { items: pagesInError }); %>
        </div>
      </div>
    <% } %>

    <% if (audits.failed.length >0) { %>
      <div class="accordion-item">
        <h2 class="accordion-header" id="accordion-header-failed-audits">
          <button class="accordion-button <%= pagesInError.length ? 'collapsed' : ''%>" type="button" data-bs-toggle="collapse" data-bs-target="#accordion-collapse-failed-audits" aria-expanded="<%= !pagesInError.length ? 'true' : 'false'%>" aria-controls="accordion-collapse-failed-audits">
            Criteri non superati
            <span class="text--fail">
              <%= results.failed_audits %></span> su
            <%= results.total_audits %>
          </button>
        </h2>
        <div id="accordion-collapse-failed-audits" class="accordion-collapse collapse <%= !pagesInError.length ? 'show' : ''%>" role="region" aria-labelledby="accordion-header-failed-audits">
          <%- include('partials/accordion-body', { items: audits.failed, label: 'Criterio' }); %>
        </div>
      </div>
    <% } %>

    <% if (audits.passed.length >0) { %>
      <div class="accordion-item">
        <h2 class="accordion-header" id="accordion-header-passed-audits">
          <button class="accordion-button <%= (pagesInError.length || audits.failed.length) ? 'collapsed' : ''%>" type="button" data-bs-toggle="collapse" data-bs-target="#accordion-collapse-passed-audits" aria-expanded="<%= !pagesInError.length && !audits.failed.length ? 'true' : 'false'%>" aria-controls="accordion-collapse-passed-audits">
            Criteri superati
            <span class="text--pass">
              <%= results.passed_audits %></span> su
            <%= results.total_audits %>
          </button>
        </h2>
        <div id="accordion-collapse-passed-audits" class="accordion-collapse collapse <%= !pagesInError.length && !audits.failed.length ? 'show' : ''%>" role="region" aria-labelledby="accordion-header-passed-audits">
          <%- include('partials/accordion-body', { items: audits.passed, label: 'Criterio' }); %>
        </div>
      </div>
    <%  } %>

    <% if (audits.info.length >0) { %>
      <div class="accordion-item">
        <h2 class="accordion-header" id="accordion-header-info-audits">
          <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#accordion-collapse-info-audits" aria-expanded="false" aria-controls="accordion-collapse-info-audits">
            Raccomandazioni non obbligatorie
          </button>
        </h2>
        <div id="accordion-collapse-info-audits" class="accordion-collapse collapse" role="region" aria-labelledby="accordion-header-info-audits">
          <%- include('partials/accordion-body', { items: audits.info, label: 'Raccomandazione' }); %>
        </div>
      </div>
    <%  } %>

    <% if ( lighthouseIFrame ) { %>
    <div class="accordion-item">
      <h2 class="accordion-header" id="accordion-header-lighthouse-audits">
        <div class="accordion-button collapsed lighthouse-button">
          Report di Lighthouse (non oggetto di verifica della misura)
          <button class="btn btn-primary" type="button" data-bs-toggle="modal" data-bs-target="#iframeModal" aria-expanded="false" aria-controls="accordion-collapse-lighthouse-audits" data-focus-mouse="false">Apri il report</button>
        </div>
      </h2>
      <div class="modal fade" id="iframeModal" tabindex="-1" aria-labelledby="iframeModalLabel" data-focus-mouse="false" style="display: none;" aria-hidden="true">
        <div class="modal-dialog modal-lg">
          <div class="modal-content">
            <div class="modal-header">
              <h2 class="modal-title h5" id="iframeModalLabel">Report di Lighthouse (non oggetto di verifica della misura)</h2>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" data-focus-mouse="false"></button>
            </div>
            <div class="modal-body">
              <iframe title="Lighthouse report" width="100%" height="500px" srcdoc="<%= lighthouseIFrame %>"></iframe>
            </div>
          </div>
        </div>
      </div>
      <% } %>
    </div>
  </div>
</main>

<%- include('partials/footer', {crawler_version}); %>

<!-- JS DEPENDENCIES -->
<script src=" https://unpkg.com/bootstrap-italia@2.9.1/dist/js/bootstrap-italia.bundle.min.js"></script>
</body>

</html>